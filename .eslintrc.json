{
    "extends": [
        "airbnb-base",
        "plugin:vue/recommended"
    ],
    "rules": {
        "no-param-reassign": [
            "error",
            {
                "props": true,
                "ignorePropertyModificationsFor": [
                "state",
                "acc",
                "e",
                "ctx",
                "req",
                "request",
                "res",
                "response",
                "$scope"
                ]
            }
        ]
    }
}